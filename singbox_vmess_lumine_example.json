{
  "log": {
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "address": "https://1.1.1.1/dns-query",
        "detour": "proxy"
      }
    ]
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "inet4_address": "172.19.0.1/28",
      "auto_route": true,
      "strict_route": true,
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "type": "socks",
      "tag": "lumine",
      "server": "127.0.0.1",
      "server_port": 1080,
      "version": "5"
    },
    {
      "type": "vmess",
      "tag": "proxy",
      "server": "your.server.com",
      "server_port": 443,
      "uuid": "your-uuid-here",
      "security": "auto",
      "alter_id": 0,
      "detour": "lumine",
      "tls": {
        "enabled": true,
        "server_name": "your.server.com",
        "insecure": false
      },
      "transport": {
        "type": "ws",
        "path": "/vmess"
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "proxy"
  }
}
